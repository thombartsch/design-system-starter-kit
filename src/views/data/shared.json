}, {
  "label": "Image",
  "value": "${ProductImageURL.medium}",
  "id": "rte-btn-3"
}, {
  "label": "URL",
  "value": "${OriginalRequestURL}",
  "id": "rte-btn-4"
}, {
  "label": "If-Then-Else",
